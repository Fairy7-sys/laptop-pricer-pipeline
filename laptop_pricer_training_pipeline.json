            },
            "componentRef": {
              "name": "comp-train-test-split"
            },
            "dependentTasks": [
              "importer"
            ],
            "inputs": {
              "artifacts": {
                "dataset": {
                  "taskOutputArtifact": {
                    "outputArtifactKey": "artifact",
                    "producerTask": "importer"
                  }
                }
              }
            },
            "taskInfo": {
              "name": "train-test-split"
            }
          }
        }
      },
      "inputDefinitions": {
