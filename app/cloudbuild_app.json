{
    "steps": [

      {
        "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
        "entrypoint": "gcloud",
        "args": [
          "run",
          "deploy",
          "laptop-pricer",
          "--image",
          "gcr.io/$PROJECT_ID/laptop-pricer:0.0.1",
          "--region",
          "us-central1",
          "--platform",
          "managed",
          "--update-env-vars",
          "PROJECT_ID=$PROJECT_ID,MODEL_REPO=model_repo_rgather,MODEL_NAME=laptop_pricer_model.pkl",
          "--allow-unauthenticated",
          "--no-cpu-throttling",
          "--memory",
          "4 Gi"
        ]
      }
    ]
  }